    <span class="nt">&lt;annotation&gt;</span>
        <span class="nt">&lt;appinfo&gt;</span>
            <span class="nt">&lt;link:linkbaseRef</span> <span class="na">xlink:type=</span><span class="s">&#39;simple&#39;</span> 
            <span class="na">xlink:href=</span><span class="s">&#39;http://xbrl.iasb.org/int/fr/ifrs/gp/2005-05-15/ifrs-gp-pre-bs-liquidity-2005-05-15.xml&#39;</span>
            <span class="na">xlink:role=</span><span class="s">&#39;http://www.xbrl.org/2003/role/presentationLinkbaseRef&#39;</span>
