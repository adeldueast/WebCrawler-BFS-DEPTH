 (function(window) {
   'use strict';
   var OPTS = {embed: true};
   var placeholder = document.getElementById('placeholder');
   placeholder.style.display = "block";
   w3capi.apiKey = 'dpykf76c9b4g8kcgg48848soskokggk';
   w3capi.authMode = 'param';
   w3capi.groups()
     .fetch(OPTS, function(err, data) {
       if (err) {
         placeholder.innerText = '(Error: groups could not be loaded.)';
         console.debug(`Error:\n${JSON.stringify(err, null, 4)}`);
       } else {
         var URL_PREFIX = /^\s*(https?:\/\/)?((www\.)?w3\.org)?\//i,
           URL_SUFFIX = /\/\s*$/,
           REGEX_BG = /business\s+group\s*$/i,
           REGEX_CG = /community\s+group\s*$/i,
           REGEX_IG = /interest\s+group\s*$/i,
           REGEX_WG = /working\s+group\s*$/i,
           ABBR_BG = '<abbr title="Business Group">BG</abbr>',
           ABBR_CG = '<abbr title="Community Group">CG</abbr>',
           ABBR_IG = '<abbr title="Interest Group">IG</abbr>',
           ABBR_WG = '<abbr title="Working Group">WG</abbr>';
         var list = document.querySelector('.lastUnit ul'),
           items = list.getElementsByTagName('li'),
           newItems = [],
           html = '';
         for (var i = 0; i < items.length; i ++)
           newItems.push({innerText: items[i].innerText.trim(), innerHTML: '<li>' + items[i].innerHTML.trim() + '</li>'});
         for (var i of data) {
           var elem = i.name.trim(),
             markup = i.name.replace(REGEX_BG, ABBR_BG).replace(REGEX_CG, ABBR_CG).replace(REGEX_IG, ABBR_IG).replace(REGEX_WG, ABBR_WG);
           if (i['_links'] && i['_links'].homepage && i['_links'].homepage.href)
             markup = `<a href="${i['_links'].homepage.href}">${markup}</a>`;
           markup = '<li>' + markup + '</li>\n';
           newItems.push({innerText: elem, innerHTML: markup});
           newItems = newItems.sort(function(a, b) {
             if (a.innerText.toLowerCase() < b.innerText.toLowerCase())
               return -1;
             else if (a.innerText.toLowerCase() > b.innerText.toLowerCase())
               return 1;
             else
               return 0;
           });
         }
         for (var i of newItems)
           html += `${i.innerHTML}\n`;
         list.innerHTML = html;
         placeholder.style.display = "none";
       }
     });
     })(window);
     //]]>
</script>

</body>

</html>
